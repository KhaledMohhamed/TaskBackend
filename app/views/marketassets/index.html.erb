<p id="notice"><%= notice %></p>

<h1>Marketassets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marketassets.each do |marketasset| %>
      <tr>
        <td><%= marketasset.name %></td>
        <td><%= marketasset.value %></td>
        <td><%= link_to 'Show', marketasset %></td>
        <td><%= link_to 'Edit', edit_marketasset_path(marketasset) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Marketasset', new_marketasset_path %>
